{
    "Chase": {
        "headers": "Details,Posting Date,Description,Amount,Type,Balance,Check or Slip #",
        "format": {
            "Posting Date": {
                "column": "Date",
                "parsing": "dateformat",
                "formatter": "M/d/yyyy"
            },
            "Description": {
                "column": "Description"
            },
            "Amount": {
                "column": "Cost",
                "parsing": "spending",
                "formatter": "normal"
            }
        }
    },
    "Capital One": {
        "headers": "Transaction Date,Posted Date,Card No.,Description,Category,Debit,Credit",
        "format" : {
            "Transaction Date": {
                "column": "Date",
                "parsing": "dateformat",
                "formatter": "yyyy-MM-dd"
            },
            "Card No.": {
                "column": "Card",
                "parsing": "int"
            },
            "Description": {
                "column": "Description"
            },
            "Category": {
                "column": "Category"
            },
            "Debit": {
                "column": "Cost",
                "parsing": "spending",
                "formatter": "inverse"
            },
            "Credit": {
                "column": "Cost",
                "parsing": "spending",
                "formatter": "normal"
            }
        }
    }
}
